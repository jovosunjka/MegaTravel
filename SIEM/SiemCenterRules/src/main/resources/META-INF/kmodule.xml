<?xml version="1.0" encoding="UTF-8"?>
<kmodule xmlns="http://jboss.org/kie/6.0.0/kmodule">
    <kbase name="SbzRulesKBase" eventProcessingMode="stream" packages="sbz.rules">
        <ksession name="SbzRulesSession" />
    </kbase>

    <kbase name="logsrules" packages="logsrules">
        <ksession name="logs-session"/>
    </kbase>

    <kbase name="loginrules" eventProcessingMode="stream" default="true" packages="loginrules">
        <ksession default="true" clockType="pseudo" name="login-session"/>
    </kbase>

    <kbase name="long_period_rules" eventProcessingMode="stream" packages="long_period_rules">
        <ksession clockType="pseudo" name="long_period_session"/>
    </kbase>
</kmodule>
